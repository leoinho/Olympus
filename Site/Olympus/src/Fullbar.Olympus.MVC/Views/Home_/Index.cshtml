<body>
    <div class="wrapper">
        <div class="container container-landing">
            <div class="row geral-animate4">
                <div class="col-md-3 pad-top-40 logo-branco hidden-xs hidden-sm">
                    <img src=@Url.Content("~/assets/img/logo-branco.png") class="img-responsive pull-left">
                </div>
                <div class="col-md-3 pad-top-20 logo-branco hidden-lg hidden-md">
                    <img src=@Url.Content("~/assets/img/logo-branco.png") class="img-responsive center-block">
                </div>
                <div class="col-md-9">
                </div>
                
            </div>
            @*<div class="row">
                <div class="col-md-4">
                </div>
                <div class="col-md-4 logo-hcp geral-animate1">
                    <img src=@Url.Content("~/assets/img/logo-hcp.png") class="img-responsive center-block">
                </div>
                <div class="col-md-4">
                </div>
            </div>*@
            @*<div class="row">
                <div class="col-md-12 pad-top-20 texto-hcp geral-animate2">
                    <h1>@Fullbar.Olympus.MVC.Resource.HomeTextoEducacao</h1>
                    <p>@Fullbar.Olympus.MVC.Resource.HomeTextoNoDNA</p>
                </div>
            </div>*@

            <div class="row lg-home">
                <div class="row geral-animate3">
                    <div class="col-md-4">
                    </div>
                    <div class="col-md-4 texto-hcp-enter">
                        @Fullbar.Olympus.MVC.Resource.HomeTextoEntrarNoSite
                    </div>
                    <div class="col-md-4">
                    </div>
                </div>

                <div class="row geral-animate3">
                    <div class="col-md-4">
                    </div>
                    <div class="col-md-4 text-center idioma-land">
                        @{
                            var ingles = System.Web.Configuration.WebConfigurationManager.AppSettings["siteEn"];
                            var espanhol = System.Web.Configuration.WebConfigurationManager.AppSettings["siteEs"];
                            var portugues = System.Web.Configuration.WebConfigurationManager.AppSettings["sitePt"];
                        }

                        <ul>
                            <li><a href="@ingles">@Fullbar.Olympus.MVC.Resource.EN</a></li>
                            <li><a href="@espanhol">@Fullbar.Olympus.MVC.Resource.ES</a></li>
                            <li><a href="@portugues">@Fullbar.Olympus.MVC.Resource.PT</a></li>
                        </ul>
                    </div>
                    <div class="col-md-4">
                    </div>

                </div>
            </div>
        </div>


        <div class="push"></div>
    </div>

    <footer class="footer bg-footer geral-animate5">
        <div class="container ">
            <div class="row">
                <div class="text-right pad-top-40 text-footer-landing">
                    <p>© Olympus 2017</p>
                </div>
            </div>
        </div>
    </footer>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
